{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "devCommand": "cd frontend && npm run dev",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_API_TIMEOUT": "@vite_api_timeout",
    "VITE_JWT_STORAGE_KEY": "@vite_jwt_storage_key",
    "VITE_GOOGLE_MAPS_API_KEY": "@vite_google_maps_api_key",
    "VITE_MAPBOX_ACCESS_TOKEN": "@vite_mapbox_access_token",
    "VITE_MAX_FILE_SIZE": "@vite_max_file_size",
    "VITE_ALLOWED_FILE_TYPES": "@vite_allowed_file_types",
    "VITE_ENABLE_VR_FEATURES": "@vite_enable_vr_features",
    "VITE_ENABLE_VIDEO_TOURS": "@vite_enable_video_tours",
    "VITE_ENABLE_CHAT": "@vite_enable_chat",
    "VITE_GOOGLE_ANALYTICS_ID": "@vite_google_analytics_id",
    "VITE_HOTJAR_ID": "@vite_hotjar_id",
    "VITE_FACEBOOK_APP_ID": "@vite_facebook_app_id",
    "VITE_TWITTER_API_KEY": "@vite_twitter_api_key",
    "VITE_STRIPE_PUBLISHABLE_KEY": "@vite_stripe_publishable_key"
  }
}